  "queries": [
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "_workspaceQuery\n| count ",
      "id": "8d31e211-faf1-431c-acc9-2722fa589e3d",
      "usedVariables": [
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "Items\n| join kind=inner _workspaceQuery on $right.id == $left.workspaceId\n| count ",
      "id": "2b4dfb1d-b8df-4e81-a08f-296c157aeb99",
      "usedVariables": [
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "Items\n| join kind=inner _workspaceQuery on $left.workspaceId == $right.id\n| where itemType in (_itemType)\n| summarize ItemsCount=count() by itemType",
      "id": "10394ab4-ea9c-4048-a125-e985c6361ad1",
      "usedVariables": [
        "_itemType",
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "let _workspaces = _workspaceQuery |project id;\nlet _timespan = case(\n    _endTime - _startTime >= timespan(730d), timespan(1d),\n    _endTime - _startTime >= timespan(365d), timespan(1d),\n    _endTime - _startTime >= timespan(60d), timespan(6h),\n    _endTime - _startTime >= timespan(14d), timespan(1h),\n    _endTime - _startTime >= timespan(1d), timespan(30m),\n    timespan(1m)\n);\nItemsViews(_endTime - _startTime)\n| where WorkspaceId in (_workspaces)\n| summarize Views=count() by ItemType,bin(CreationTime,_timespan)",
      "id": "9e298314-ba72-4b2c-80e3-fb7c09f3c7f4",
      "usedVariables": [
        "_endTime",
        "_startTime",
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "WorkspacesUsers\n| join kind=inner _workspaceQuery on $left.workspaceId == $right.id\n| summarize Users=count_distinct(graphId) by principalType,Access=groupUserAccessRight",
      "id": "1ac1b783-a290-409d-8b7c-00a1c17bc4b5",
      "usedVariables": [
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "ItemsUsers\n| join kind=inner _workspaceQuery on $left.workspaceId == $right.id\n| summarize Users=count_distinct(graphId) by Type = itemType, Access=userAccessRight",
      "id": "3998fa47-d3a4-49d6-9755-de47fad8b691",
      "usedVariables": [
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "_workspaceQuery\n| summarize Workspaces=count() by Type=type,[\"Is On Capacity\"]=isOnDedicatedCapacity\n| order by Type, ['Is On Capacity'] desc ",
      "id": "96925004-7e6e-4393-9639-0c0962fd60eb",
      "usedVariables": [
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "let _itemCount = Items | where itemType in (_itemType) | summarize Items=count() by workspaceId;\n_workspaceQuery\n| lookup Capacities on $left.capacityId == $right.CapacityId\n| lookup _itemCount on $left.id == $right.workspaceId\n| project [\"Workspace Name\"]=name, Description=description, [\"Capacity Name\"] = CapacityName, Items\n| top 1000 by Items",
      "id": "42391191-a0cf-482a-87eb-6feaab70c471",
      "usedVariables": [
        "_itemType",
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "let _workspaces = _workspaceQuery | project id;\nItemsViews(_endTime - _startTime)\n| where WorkspaceId in (_workspaces)\n| project IPLocation=geo_info_from_ip_address(tostring(details.ClientIP)), ItemType\n| project latitude=toreal(IPLocation.latitude), longitude=toreal(IPLocation.longitude), ItemType\n| summarize Events=count() by latitude,longitude,ItemType\n| where isnotempty( latitude) or isnotempty(longitude)",
      "id": "a2987918-351a-4e83-9bb3-f4a88b23f3d6",
      "usedVariables": [
        "_endTime",
        "_startTime",
        "_workspaceQuery"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "_tenantSettingBase\n| project TenantSettingGroup,Title, Enabled, CanSpecifySecurityGroups,EnabledSecurityGroups\n| order by TenantSettingGroup asc ",
      "id": "719e4955-58b1-450d-bf42-82a007d181b5",
      "usedVariables": [
        "_tenantSettingBase"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "_tenantSettingBase\n| summarize Settings=count() by Enabled",
      "id": "71f1e5cf-6f4b-4279-ad25-d5f6cbb59f49",
      "usedVariables": [
        "_tenantSettingBase"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "WorkspaceDelegatedSettings\n| count ",
      "id": "9c159e0e-3009-4bc1-818f-5743fd3bf21b",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "CapacityDelegatedSettings\n| summarize count() by CapacityId\n| lookup Capacities on CapacityId\n| where isnotempty( CapacityName)\n| project ['Settings Changed']=count_, ['Capacity Name']=CapacityName\n| order by ['Settings Changed']",
      "id": "867e916a-1493-4e2e-a3a5-c5a6bad67f98",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "DomainDelegatedSettings\n| count",
      "id": "b8c58d2b-76a2-41d7-ac52-09b4c242ba7d",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "_tenantSettingBase\n| count ",
      "id": "cf5690e2-b5b5-4bc1-b69b-7c614f9696ea",
      "usedVariables": [
        "_tenantSettingBase"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "TenantSettingsHistory\n| where scanTime between (datetime_add('second',-1,_date) .. datetime_add('second',+1,_date))\n| where Enabled in (_tenantSettingEnabled)",
      "id": "e37731ac-8504-46df-b9f8-dfd7e9d8e0e1",
      "usedVariables": [
        "_date",
        "_tenantSettingEnabled"
      ]
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "8a9a70d8-02b9-4236-85be-90fa83a60c8e"
      },
      "text": "TenantSettingsHistory\n| where scanTime between (datetime_add('second',-1,_date) .. datetime_add('second',+1,_date))\n| where Enabled in (_tenantSettingEnabled)\n| count ",
      "id": "f22570bb-1b98-4b8c-9132-f9a8702503c7",
      "usedVariables": [
        "_date",
        "_tenantSettingEnabled"
      ]
    },
